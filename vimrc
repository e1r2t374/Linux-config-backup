set number
syntax on
set hlsearch
set mouse=a
set showmatch
set visualbell t_vb=
set ignorecase
set list
set title
set cursorline
set clipboard=unnamedplus
highlight Visual cterm=italic ctermbg=yellow ctermfg=black guibg=#ffcc00 guifg=#000000
highlight Search cterm=bold ctermbg=white ctermfg=black guibg=#ffffff guifg=#000000
highlight MatchParen cterm=italic ctermbg=blue ctermfg=black guibg=#5f87af guifg=#000000
hi CursorLine term=NONE cterm=NONE gui=NONE guibg=#FFFFFF ctermbg=NONE
